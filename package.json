{
  "name": "social-media",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://cxc421.github.io/social-media/build/",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "onchange": "^3.2.1",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.2",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.10"
  },
  "scripts": {
    "build-scss": "node-sass --output-style expanded src/scss/index.scss src/index.build.css",
    "watch-scss": "onchange \"src/scss/**/*.scss\" -- npm run build-scss",
    "start-js": "react-scripts start",
    "start": "npm run build-scss && run-p watch-scss start-js",
    "build": "npm run build-scss && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
